{
  "name": "orchid/press",
  "description": "Press package for Orchid Platform",
  "keywords": [
    "press",
    "cms",
    "blog",
    "orchid",
    "platform",
    "laravel"
  ],
  "license": "MIT",
  "type": "library",
  "homepage": "https://orchid.software",
  "authors": [
    {
      "name": "Alexandr Chernyaev",
      "email": "bliz48rus@gmail.com"
    },
    {
      "name": "Pavel Salador",
      "email": "Salador2005@yandex.ru"
    }
  ],
  "require": {
    "cviebrock/eloquent-sluggable": "^6.0",
    "orchid/platform": "^6.0|dev-master"
  },
  "require-dev": {
    "fzaninotto/faker": "^1.4",
    "mockery/mockery": "~1.0",
    "orchestra/testbench": "~4.0",
    "phpunit/php-code-coverage": "^7.0",
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-4": {
        "Orchid\\Database\\Press\\Seeds\\": "database/seeds/",
        "Orchid\\Press\\": "src/"
    },
    "files": [
      "src/Support/helpers.php"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "Orchid\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Orchid\\Press\\Providers\\PressServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  }
}
